{
  "root": true,
  "env": {
    "es2022": true
  },
  "overrides": [
    {
      "files": ["*.tsx?"],
      "excludedFiles": ["*.d.ts"],
      "plugins": ["@typescript-eslint"],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
      }
    }
  ]
}
