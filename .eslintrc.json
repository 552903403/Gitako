{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "no-const-assign": "warn",
    "no-this-before-super": "error",
    "no-undef": "error",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "valid-typeof": "warn",
    "babel/new-cap": 1,
    "babel/no-invalid-this": 1,
    "babel/object-curly-spacing": 0,
    "babel/quotes": 0,
    "babel/semi": 0,
    "babel/no-unused-expressions": 1
  },
  "plugins": [
    "babel",
    "react"
  ]
}
