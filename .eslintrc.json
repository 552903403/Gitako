{
  "parser": "babel-eslint",
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "no-const-assign": 1,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "constructor-super": 1,
    "valid-typeof": 1,
    "babel/new-cap": 1,
    "babel/no-invalid-this": 1,
    "babel/object-curly-spacing": 0,
    "babel/quotes": 0,
    "babel/semi": 0,
    "babel/no-unused-expressions": 1
  },
  "plugins": [
    "babel"
  ],
  "globals": {
    "chrome": true
  }
}
