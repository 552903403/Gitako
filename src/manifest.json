{
  "manifest_version": 2,
  "name": "Gitako",
  "icons": {
    "64": "icons/Gitako-64.png",
    "128": "icons/Gitako-128.png",
    "256": "icons/Gitako-256.png"
  },
  "permissions": ["storage", "<all_urls>"],
  "web_accessible_resources": ["icons/vscode/*", "content.css"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["firefox-shim.js", "browser-polyfill.js", "content.js"]
    }
  ]
}
